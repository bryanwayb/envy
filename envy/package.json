{
  "name": "envy",
  "version": "0.0.0",
  "description": "envy",
  "main": "app.js",
  "author": {
    "name": ""
  },
  "bin": {
    "envy": "app.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "watch": "tsc --watch",
    "dist": "tsc --outDir ./dist/build && node ./dist/build/prepareBuild.js && pkg ./dist/build/app.js -c ./package.json"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.14.7",
    "@types/yargs": "^17.0.15",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "console-table-printer": "^2.11.1",
    "handlebars": "^4.7.7",
    "i": "^0.3.7",
    "js-yaml": "^4.1.0",
    "npm": "^9.1.3",
    "pkg": "^5.8.0",
    "reflect-metadata": "^0.1.13",
    "spinnies": "^0.5.1",
    "typedi": "^0.10.0",
    "yargs": "^17.6.2"
  },
  "pkg": {
    "assets": [
      "**/config.yml"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist/executables"
  }
}
